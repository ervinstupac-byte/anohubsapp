{
  "name": "anohub-hpp-risk-excellence-platform",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "homepage": "https://anohubs.com",
  "scripts": {
    "dev": "npm run i18n:sync && vite",
    "lint": "tsc --noEmit",
    "build:verify": "npx tsx scripts/verify_env.ts",
    "build": "npm run build:verify && npm run i18n:sync && npm run test && tsc && vite build",
    "preview": "vite preview",
    "test": "vitest run",
    "i18n:sync": "node -e \"const fs=require('fs'); try { fs.copyFileSync('src/i18n/locales/en.json', 'src/i18n/en.json'); fs.copyFileSync('src/i18n/locales/bs.json', 'src/i18n/bs.json'); console.log('i18n synced'); } catch(e) { console.error(e); process.exit(1); }\"",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist"
  },
  "dependencies": {
    "@google/generative-ai": "^0.1.1",
    "@react-three/drei": "^9.99.0",
    "@react-three/fiber": "^8.15.16",
    "@supabase/supabase-js": "^2.39.3",
    "@types/three": "^0.160.0",
    "decimal.js": "^10.6.0",
    "framer-motion": "^12.23.26",
    "html2canvas": "^1.4.1",
    "i18next": "^23.8.2",
    "jspdf": "^2.5.2",
    "jspdf-autotable": "^3.8.2",
    "leaflet": "^1.9.4",
    "lucide-react": "^0.330.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^14.0.5",
    "react-leaflet": "^4.2.1",
    "react-router-dom": "^6.22.0",
    "recharts": "^2.12.0",
    "three": "^0.160.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.10",
    "@types/leaflet": "^1.9.8",
    "@types/node": "^20.11.16",
    "@types/react": "^18.2.55",
    "@types/react-dom": "^18.2.19",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.17",
    "gh-pages": "^6.1.1",
    "jsdom": "^27.3.0",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.2.2",
    "vite": "^5.1.0",
    "vitest": "^1.2.2"
  }
}